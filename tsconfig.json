{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [{ "name": "next" }]
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      "typings.d.ts",
      "src"
, "tsconfig.paths.json"    ],
    "exclude": ["node_modules"],
    "extends": "./tsconfig.paths.json" 
  }